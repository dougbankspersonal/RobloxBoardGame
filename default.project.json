        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "$ignoreUnknownInstances": true,
            
            "RobloxBoardGameShared": {
                "$className": "Folder",
                "$ignoreUnknownInstances": true,
                "$path": "src/Shared"
            }
        },
        
        "ServerScriptService": {
            "$className": "ServerScriptService",
            "$ignoreUnknownInstances": true,
            
            "RobloxBoardGameServer": {
                "$className": "Folder",
                "$ignoreUnknownInstances": true,
                "$path": "src/Server"
            }
        },
        
        "StarterPlayer": {
